Results([]);\n  }\n\n  const isMaxNominations = Object.keys(nominations).length < 5 ? false : true;\n\n  const InputPlaceholder =\n    Object.keys(nominations).length < 5\n      ? \"Search Movie\"\n      : \"All nominations has been selected\";\n\n  return (\n    <HomeContainer>\n      <img src={clappingBoard} alt=\"clapping board\" width=\"100%\" />\n      <ClapperInfo>\n        <Nav>\n          <ClapperText margin={\"20px 0 5px 0\"}>THE SHOPPIES</ClapperText>\n          <RedirectLink whereTo={\"Nominee\"} toLink={\"/nominees\"} />\n        </Nav>\n        <SearchContainer>\n          <img src={search} alt=\"search icon\" width=\"80\" />\n          <SearchBar\n            type=\"text\"\n            placeholder={InputPlaceholder}\n            value={searchInput}\n            onChange={(e) => handleChange(e)}\n            disabled={isMaxNominations}\n          />\n          <Button onClick={clearSearchInput}>\n            <img src={clearIcon} alt=\"clear icon\" width=\"80\" />\n          </Button>\n        </SearchContainer>\n        {!isMaxNominations ? (\n          <MovieResults\n            searchResults={searchResults}\n            setNominations={setNominations}\n            isMaxNominations={isMaxNominations}\n          />\n        ) : (\n          <MaxNominationsReached />\n        )}\n      </ClapperInfo>\n    </HomeContainer>\n  );\n};\n\nexport default Home;\n","/Users/jinazhu/repos/large-projects/the-shoppies/src/pages/Nominees/index.js",[],"/Users/jinazhu/repos/large-projects/the-shoppies/src/config.js",[],"/Users/jinazhu/repos/large-projects/the-shoppies/src/pages/Home/styled.js",["80"],"import styled, { css } from \"styled-components\";\n\nexport const HomeContainer = styled.div`\n  background-color: black;\n  background-image: url(data:image/png;base64,);\n  height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: flex-start;\n  flex-direction: column;\n`;\n\nexport const ClapperInfo = styled.div`\n  display: flex;\n  align-items: f